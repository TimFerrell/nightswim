# Hayward OmniLogic Credentials
# Copy this file to .env and fill in your actual credentials
# These are required for the application to work
HAYWARD_USERNAME=your-email@example.com
HAYWARD_PASSWORD=your-password

# Optional: CORS Origin (defaults to http://localhost:3000)
CORS_ORIGIN=http://localhost:3000

# Optional: Session Secret (auto-generated if not provided)
SESSION_SECRET=your-session-secret-here

# Optional: Node Environment (development/production)
NODE_ENV=development 