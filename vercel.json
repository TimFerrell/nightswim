{
  "version": 2,
  "name": "nightswim",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/collect-all",
      "schedule": "0 12 * * *"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 